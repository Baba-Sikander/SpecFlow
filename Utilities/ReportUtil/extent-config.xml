<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
  <configuration>
    <!-- report theme -->
    <!-- standard, dark -->
    <theme>standard</theme>

    <!-- document encoding -->
    <!-- defaults to UTF-8 -->
    <encoding>UTF-8</encoding>

    <!-- protocol for script and stylesheets -->
    <!-- defaults to https -->
    <protocol>defaults</protocol>

    <!-- title of the document -->
    <documentTitle>Pegasus Automation</documentTitle>

    <chartVisibilityOnOpen>true</chartVisibilityOnOpen>

    <!-- report name - displayed at top-nav -->
    <reportName>Execution Report</reportName>

    <!-- location of charts in the test view -->
    <!-- top, bottom -->
    <testViewChartLocation>bottom</testViewChartLocation>
	  <!-- report name - displayed at top-nav -->
	  <reportName>
		  <![CDATA[
            <img src='.//render//gac.png' />
        ]]>
	  </reportName>
    <!-- custom javascript -->
    <scripts>
      <![CDATA[
                $(document).ready(function() {
                    
                });
            ]]>
    </scripts>

    <!-- custom styles -->
    <styles>
      <![CDATA[
              .report-name { padding-left: 10px; } .report-name > img { float: 
      left;height: 90%;margin-left: 30px;margin-top: 2px;width: auto; }  
            ]]>
    </styles>
  </configuration>
</extentreports>